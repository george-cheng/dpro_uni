<view class="tranContract mainBox data-v-598f5cbe"><view class="tractCon data-v-598f5cbe"><view class="enConList data-v-598f5cbe"><block wx:for="{{contractList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="enList data-v-598f5cbe"><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">合约名称</text><text class="data-v-598f5cbe">{{item.name}}</text></view><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">抢购币种名称</text><text class="data-v-598f5cbe">{{item.coin_name}}</text></view><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">抢购标准</text><text class="data-v-598f5cbe">{{item.ded_amount}}</text></view><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">总产量</text><text class="data-v-598f5cbe">{{item.ded_amount*item.out_mult}}</text></view><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">释放周期</text><text class="data-v-598f5cbe">{{item.return_days}}</text></view><view class="data-v-598f5cbe"><text class="data-v-598f5cbe">开放时间</text><text class="data-v-598f5cbe">{{item.open_date}}</text></view><view class="state data-v-598f5cbe"><text class="data-v-598f5cbe">状态</text><text class="{{['data-v-598f5cbe',item.state===0?'close':'open']}}">{{item.state===0?'关闭':'开启'}}</text></view></view></block></view><view class="tractBtn data-v-598f5cbe"><view data-event-opts="{{[['tap',[['tranctBtnEvent',[0]]]]]}}" bindtap="__e" class="data-v-598f5cbe">立即抢购</view><view data-event-opts="{{[['tap',[['tranctBtnEvent',[1]]]]]}}" bindtap="__e" class="data-v-598f5cbe">委托抢购</view><view data-event-opts="{{[['tap',[['tranctBtnEvent',[2]]]]]}}" bindtap="__e" class="data-v-598f5cbe">合约资产</view><view data-event-opts="{{[['tap',[['tranctBtnEvent',[3]]]]]}}" bindtap="__e" class="data-v-598f5cbe">抢购收益</view><view data-event-opts="{{[['tap',[['tranctBtnEvent',[4]]]]]}}" bindtap="__e" class="data-v-598f5cbe">分享收益</view></view></view></view>