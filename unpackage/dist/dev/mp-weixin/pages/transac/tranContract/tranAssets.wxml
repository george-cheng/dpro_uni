<view class="tranContract mainBox data-v-b2d3355c"><view class="tractCon data-v-b2d3355c"><view class="enConList data-v-b2d3355c"><block wx:for="{{recordList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['tranAssetsEvent',['$0'],[[['recordList','id',item.id]]]]]]]}}" class="enList data-v-b2d3355c" bindtap="__e"><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">合约名称</text><text class="data-v-b2d3355c">{{item.treaty_name}}</text></view><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">抢购标准</text><text class="data-v-b2d3355c">{{item.ded_amount}}</text></view><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">总产量</text><text class="data-v-b2d3355c">{{item.ded_amount*item.out_mult}}</text></view><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">剩余天数</text><text class="data-v-b2d3355c">{{item.return_days+" 天"}}</text></view><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">抢购时间</text><text class="data-v-b2d3355c">{{item.purch_date}}</text></view><view class="data-v-b2d3355c"><text class="data-v-b2d3355c">抢购状态</text><text class="{{['data-v-b2d3355c',item.purch_state===0?'close':'open']}}">{{item.purch_state===0?'失败':'成功'}}</text></view><view class="state data-v-b2d3355c"><text class="data-v-b2d3355c">激活状态</text><text data-event-opts="{{[['tap',[['deactivaEvent',['$0'],[[['recordList','id',item.id]]]]]]]}}" class="{{['data-v-b2d3355c',item.activate_state===0?'close':'open']}}" catchtap="__e">{{item.activate_state===1?'已激活':'去激活'}}</text></view></view></block></view></view></view>